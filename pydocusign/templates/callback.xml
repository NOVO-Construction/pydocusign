{# Jinja2 #}<?xml version="1.0" encoding="utf-8"?>
<!-- Sample body of the request posted by DocuSign as event notification when all signers signed documents, during embedded signing workflow. -->
<DocuSignEnvelopeInformation xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.docusign.net/API/3.0">
  <EnvelopeStatus>
    <RecipientStatuses>
      {% for signer in signers %}
        <RecipientStatus>
          <Type>Signer</Type>
          <Email>{{ signer.Email }}</Email>
          <UserName>{{ signer.UserName }}</UserName>
          <RoutingOrder>{{ signer.RoutingOrder|default(loop.index) }}</RoutingOrder>
          {% if signer.Sent %}<Sent>{{ signer.Sent|default('2014-10-06T00:58:28.98') }}</Sent>{% endif %}
          {% if signer.Delivered %}<Delivered>{{ signer.Delivered|default('2014-10-06T00:58:30.98') }}</Delivered>{% endif %}
          {% if signer.Signed %}<Signed>{{ signer.Signed|default('2014-10-06T00:58:35.98') }}</Signed>{% endif %}
          {% if signer.Declined or signer.DeclineReason %}<Declined>{{ signer.Declined|default('2014-10-06T00:58:35.98') }}</Declined>{% endif %}
          {% if signer.DeclineReason %}
            <DeclineReason xsi:nil="true" />
          {% else %}>
            <DeclineReason>{{ signer.DeclineReason }}</DeclineReason>
          {% endif %}
          <Status>{{ signer.Status|default('Sent') }}</Status>
          <RecipientIPAddress />
          {% if signer.ClientUserId %}<ClientUserId>{{ signer.ClientUserId }}</ClientUserId>{% endif %}
          <CustomFields />
          {% if signer.Signed %}
            <TabStatuses>
              <TabStatus>
                <TabType>SignHere</TabType>
                <Status>Signed</Status>
                <XPosition>209</XPosition>
                <YPosition>388</YPosition>
                <DocumentID>1</DocumentID>
                <PageNumber>1</PageNumber>
              </TabStatus>
            </TabStatuses>
          {% endif %}
          <AccountStatus>Active</AccountStatus>
          {% if signer.Delivered or signer.Signed %}
            <EsignAgreementInformation>
              <AccountEsignId>{{ signer.AccountEsignId|default('f32da517-df75-4497-9b9d-6a6270329b1%d'|format(loop.index0)) }}</AccountEsignId>
              <UserEsignId>{{ signer.UserEsignId|default('cbe72a25-51fb-4208-9a83-6403b81fa2d%d'|format(loop.index0)) }}</UserEsignId>
              <AgreementDate>{{ signer.AgreementDate|default('2014-10-06T00:58:30.98') }}</AgreementDate>
            </EsignAgreementInformation>
          {% endif %}
          <RecipientId>{{ signer.RecipientId|default('81077740-807b-4a1b-9af3-74f79544d81%d'|format(loop.index0)) }}</RecipientId>
        </RecipientStatus>
      {% endfor %}
    </RecipientStatuses>
    <TimeGenerated>{{ TimeGenerated|default('2014-10-06T00:58:49.1655913') }}</TimeGenerated>
    <EnvelopeID>{{ Status|default('dffa5826-c5a8-423a-ab31-3e2fafd01c58') }}</EnvelopeID>
    <Subject>{{ Subject }}</Subject>
    <UserName>{{ UserName }}</UserName>
    <Email>{{ Email }}</Email>
    <Status>{{ Status|default('Sent') }}</Status>
    <Created>{{ Created|default('2014-10-06T00:58:28.713') }}</Created>
    <Sent>{{ Sent|default('2014-10-06T00:58:29.043') }}</Sent>
    {% if Delivered %}<Delivered>{{ Sent|default('2014-10-06T00:58:29.043') }}</Delivered>{% endif %}
    {% if Signed %}<Signed>{{ Sent|default('2014-10-06T00:58:29.043') }}</Signed>{% endif %}
    {% if Completed or Status=='Completed' %}<Completed>{{ Sent|default('2014-10-06T00:58:29.043') }}</Completed>{% endif %}
    {% if Declined %}<Declined>{{ Declined|default('2014-10-06T00:58:50.043') }}</Declined>{% endif %}
    <ACStatus>Original</ACStatus>
    <ACStatusDate>{{ ACStatusDate|default('2014-10-06T00:58:28.713') }}</ACStatusDate>
    <ACHolder>{{ UserName }}</ACHolder>
    <ACHolderEmail>{{ Email }}</ACHolderEmail>
    <ACHolderLocation>DocuSign</ACHolderLocation>
    <SigningLocation>Online</SigningLocation>
    <SenderIPAddress>{{ SenderIpAddress|default('109.190.107.150') }}</SenderIPAddress>
    <EnvelopePDFHash />
    <CustomFields>
      <CustomField>
        <Name>AccountId</Name>
        <Show>false</Show>
        <Required>false</Required>
        <Value>{{ AccountId }}</Value>
        <CustomFieldType>Text</CustomFieldType>
      </CustomField>
      <CustomField>
        <Name>AccountName</Name>
        <Show>false</Show>
        <Required>false</Required>
        <Value>{{ AccountName }}</Value>
        <CustomFieldType>Text</CustomFieldType>
      </CustomField>
      <CustomField>
        <Name>AccountSite</Name>
        <Show>false</Show>
        <Required>false</Required>
        <Value>{{ AccountSite|default('demo') }}</Value>
        <CustomFieldType>Text</CustomFieldType>
      </CustomField>
    </CustomFields>
    <AutoNavigation>true</AutoNavigation>
    <EnvelopeIdStamping>true</EnvelopeIdStamping>
    <AuthoritativeCopy>false</AuthoritativeCopy>
    <DocumentStatuses>
      <DocumentStatus>
        <ID>1</ID>
        <Name>signatures/test.pdf</Name>
        <TemplateName />
        <Sequence>1</Sequence>
      </DocumentStatus>
    </DocumentStatuses>
  </EnvelopeStatus>
  <TimeZone>Pacific Standard Time</TimeZone>
  <TimeZoneOffset>-7</TimeZoneOffset>
</DocuSignEnvelopeInformation>
